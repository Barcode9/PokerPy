class Card:
    def __init__(self, value:int, suit:int) -> None: ...
    @property
    def value(self) -> int: ...
    @property
    def suit(self) -> int: ...
    @classmethod
    def from_string(card_str:str) -> Card: ...


class Hand:
    def __init__(self, hand_type:int, Cards:list[Card]) -> None: ...
    @property
    def hand_type(self) -> int: ...
    @property
    def Cards(self) -> list[Card]: ...
    def hand_heuristic(self) -> int: ...


def get_best_hand(cards:list[Card]) -> Hand: ...
def calculate_hand_frequency(cards:list[list[Card]]) -> list[dict[str,int]]: ...
def nice_print_frequencies(frecs:list[dict[str,int]]) -> None: ...
